    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.13",
    "@types/three": "^0.166.0",
    "dat.gui": "^0.7.9",
    "three": "^0.166.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
